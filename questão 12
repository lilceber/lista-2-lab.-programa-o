#include <stdio.h>
#include <math.h>
int main(){
int x,i,resultado;

printf("insira um número para ver a sua tabuada: ");
 scanf("%d",&x);
 
    printf("tabuada de %d é:\n"); 
    
  for(i = 1;i >= 1 && i <= 10;i++){
    resultado = x * i;
    printf("\n%d x %d = %d",x,i,resultado);
 }
    return 0;
}
